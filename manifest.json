{
  "manifest_version": 2,
 
  "name": "开启简书查看图片",
  "description": "开启简书查看图片",
  "version": "1.0",
 
  "icons": {
    "128" : "icon.png"
  },
  "permissions": [
    "tabs", "http://*/*","https://*/*"
  ],
  "content_scripts": [
    {"js":["lib/jquery.min.js","src/go.js"],"matches":[
        "https://www.jianshu.com/p/*"
    ]}
  ]
}